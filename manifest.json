{
  "manifest_version": 2,

  "name": "Quora Download",
  "description": "This extension allows to download Quora Question and Answers.",
  "version": "1.2",

  
  "content_scripts": [ {
      "matches": ["http://www.quora.com/*","https://www.quora.com/*"],
      "js":["jquery.js","jspdf.min.js","handler.js"]
      
  }],
  
 
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}
